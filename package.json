{
  "name": "create-simple-boilerplate",
  "version": "0.0.1",
  "description": "simple boilerplate",
  "license": "MIT",
  "author": "Anton Fedorovskyi <anton.fedorovsky@gmail.com>",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "eslint": "eslint",
    "eslint:--fix": "eslint --fix ./src/js/*",
    "fix": "npm run eslint:--fix && npm run stylelint:--fix",
    "start": "webpack-dev-server --config webpack.dev.js",
    "stylelint": "stylelint ./**/*.css !./dist/**/*.css",
    "stylelint:--fix": "stylelint ./**/*.css !./dist/**/*.css --fix"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 11"
  ],
  "dependencies": {
    "jquery": "^3.5.1",
    "swiper": "^5.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.0.0",
    "glob": "^7.1.6",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "4",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^3.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=3"
  }
