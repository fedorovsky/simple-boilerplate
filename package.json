    "open-browser-webpack-plugin": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.1",
    "style-loader": "^1.2.1",
